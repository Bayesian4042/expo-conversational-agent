{
  "name": "expo-nativewind-conversational-agent",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "android:reverse:3000": "adb reverse tcp:3000 tcp:3000",
    "ios": "expo run:ios",
    "ios:device": "expo run:ios -d",
    "web": "expo start --web",
    "start:dev-client": "pnpm expo:prebuild && expo start --dev-client",
    "expo:prebuild": "expo prebuild --skip-dependency-update react-native,react",
    "expo:prebuild:android:clean": "npx expo prebuild -p android --clean",
    "expo:prebuild:ios:clean": "npx expo prebuild -p ios --clean",
    "clean:native": "pnpm clean && rm -rf ./android && rm -rf ./ios && rm -rf ./.expo",
    "clean:node-modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "clean:all": "pnpm clean:native && pnpm clean:node-modules",
    "eas:build:dev:simulator:ios:local": "cross-env NODE_ENV=development APP_ENV=development eas build --profile development:simulator --platform ios --local",
    "eas:build:dev:emulator:android": "cross-env NODE_ENV=development APP_ENV=development eas build --profile development:emulator --platform android",
    "eas:build:dev:android": "cross-env NODE_ENV=development APP_ENV=development eas build --profile development --platform android",
    "eas:build:staging:android": "cross-env NODE_ENV=staging APP_ENV=staging eas build --profile staging --platform android",
    "eas:build:dev:simulator:ios": "cross-env NODE_ENV=development APP_ENV=development eas build --profile development:simulator --platform ios",
    "eas:build:dev:ios": "cross-env NODE_ENV=development APP_ENV=development eas build --profile development --platform ios",
    "eas:build:dev:device:ios": "cross-env NODE_ENV=development APP_ENV=development eas build --profile development:device --platform ios",
    "eas:build:dev:device:android": "cross-env NODE_ENV=development APP_ENV=development eas build --profile development:device --platform android",
    "eas:build:staging:ios": "cross-env NODE_ENV=staging APP_ENV=staging eas build --profile staging --platform ios"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.68",
    "@ai-sdk/react": "^2.0.95",
    "@config-plugins/react-native-webrtc": "^13.0.0",
    "@elevenlabs/react-native": "^0.5.2",
    "@expo/html-elements": "^0.13.7",
    "@expo/vector-icons": "^15.0.3",
    "@gorhom/bottom-sheet": "^5.1.1",
    "@livekit/react-native": "^2.9.5",
    "@livekit/react-native-expo-plugin": "^1.0.1",
    "@livekit/react-native-webrtc": "^137.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/hooks": "2.4.7-canary.89.248.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@rn-primitives/avatar": "^1.2.0",
    "@rn-primitives/portal": "^1.3.0",
    "@rn-primitives/slot": "^1.2.0",
    "ai": "^5.0.95",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "~54.0.25",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.18",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-picker": "^17.0.8",
    "expo-linking": "~8.0.9",
    "expo-navigation-bar": "^5.0.9",
    "expo-router": "~6.0.15",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "livekit-client": "^2.16.0",
    "lottie-react-native": "^7.3.4",
    "lucide-react-native": "^0.554.0",
    "nativewind": "4.1.23",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.2",
    "react-native-worklets": "0.5.1",
    "sonner": "^1.5.0",
    "sonner-native": "^0.14.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.12",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.39.1",
    "eslint-config-expo": "~10.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
